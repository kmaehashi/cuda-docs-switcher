{
  "manifest_version": 3,
  "name": "CUDA Docs Switcher",
  "version": "0.0.2",

  "description": "Access CUDA documentation and switch between versions quickly & easily.",
  "icons": {
    "128": "icon.png"
  },

  "action": {
    "default_icon": {
      "128": "icon.png"
    },
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "alarms", "storage"]
}
